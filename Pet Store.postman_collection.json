        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "const updatedPetName = pm.variables.replaceIn('{{$randomFirstName}}' + pm.collectionVariables.get('petId'));\r",
              "pm.collectionVariables.set('updatedPetName', updatedPetName);\r",
              "\r",
              "const availableForUpdate = JSON.parse(pm.collectionVariables.get(\"statuses\")).filter(s => s !== pm.collectionVariables.get(\"petStatus\"));\r",
              "const updatedPetStatus = availableForUpdate[\r",
              "  Math.floor(Math.random() * availableForUpdate.length)\r",
              "];\r",
              "pm.collectionVariables.set(\"updatedPetStatus\", updatedPetStatus);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
