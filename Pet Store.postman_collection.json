    {
      "name": "Get pet by ID",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", () => {\r",
              "  pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Response body is valid JSON\", () => {\r",
              "  pm.response.to.be.json;\r",
              "});\r",
              "\r",
              "const json = pm.response.json();\r",
              "\r",
              "pm.test(\"Returned id matches expected id\", () => {\r",
              "  pm.expect(String(json.id)).to.eql(String(pm.collectionVariables.get(\"petId\")));\r",
              "});\r",
              "\r",
              "pm.test(\"Returned name matches petName\", () => {\r",
              "  pm.expect(json.name).to.eql(pm.collectionVariables.get(\"petName\"));\r",
              "});\r",
              "\r",
              "\r",
              "// Verifying JSON schema\r",
              "const schema = {\r",
              "  type: \"object\",\r",
              "  required: [\"id\", \"category\", \"name\", \"photoUrls\", \"tags\", \"status\"],\r",
              "  properties: {\r",
              "    id: { type: \"number\" },\r",
              "    category: {\r",
              "        type: \"object\",\r",
              "        required: [\"id\", \"name\"],\r",
              "        properties: {\r",
              "            id: { type: \"number\" },\r",
              "            name: {type: \"string\"}\r",
              "        }\r",
              "    },\r",
              "    name: { type: \"string\" },\r",
              "    photoUrls: {\r",
              "      type: \"array\",\r",
              "      items: { type: \"string\" }\r",
              "    },\r",
              "    tags: {\r",
              "      type: \"array\",\r",
              "      items: {\r",
              "        type: \"object\",\r",
              "        required: [\"id\", \"name\"],\r",
              "        properties: {\r",
              "          id: { type: \"number\" },\r",
              "          name: { type: \"string\" }\r",
              "        }\r",
              "      }\r",
              "    },\r",
              "    status: {\r",
              "      type: \"string\",\r",
              "      enum: [\"available\",\"pending\",\"sold\"]\r",
              "    }\r",
              "  }\r",
              "};\r",
              "\r",
              "pm.test(\"Schema is valid\", () => {\r",
              "  pm.response.to.have.jsonSchema(schema);\r",
              "});\r",
              "\r",
              "pm.test(\"Response time is under 1500ms\", () => {\r",
              "  pm.expect(pm.response.responseTime).to.be.below(1500);\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
