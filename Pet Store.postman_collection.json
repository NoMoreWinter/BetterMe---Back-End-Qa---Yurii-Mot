  "item": [
    {
      "name": "Create pet",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", () => {\r",
              "  pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Response body is valid JSON\", () => {\r",
              "  pm.response.to.be.json;\r",
              "});\r",
              "\r",
              "const json = pm.response.json();\r",
              "\r",
              "pm.test(\"id matches petId\", () => {\r",
              "  pm.expect(String(json.id)).to.eql(String(pm.collectionVariables.get(\"petId\")));\r",
              "});\r",
              "\r",
              "pm.test(\"name matches petName\", () => {\r",
              "  pm.expect(json.name).to.eql(pm.collectionVariables.get(\"petName\"));\r",
              "});\r",
              "\r",
              "pm.test(\"status is one of allowed\", () => {\r",
              "  const allowed = [\"available\", \"pending\", \"sold\"];\r",
              "  pm.expect(allowed).to.include(json.status);\r",
              "});\r",
              "\r",
              "pm.test(\"Response time is under 1500ms\", () => {\r",
              "  pm.expect(pm.response.responseTime).to.be.below(1500);\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "const petId = Math.floor(Math.random() * 10000); // Generate a random petId and assign it to the petId variable in the collection\r",
              "pm.collectionVariables.set('petId', petId);\r",
              "\r",
              "const petName = pm.variables.replaceIn('{{$randomFirstName}}'); // Generate a radom name for a pet and assign it to the petName variable in the collection\r",
              "pm.collectionVariables.set('petName', petName);\r",
              "\r",
              "const statuses = ['available', 'pending', 'sold']; // Choose random status from array and assign it to the petStatus variable in the collection\r",
              "pm.collectionVariables.set('statuses', JSON.stringify(statuses));\r",
              "const petStatus = statuses[Math.floor(Math.random() * statuses.length)];\r",
              "pm.collectionVariables.set('petStatus', petStatus);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Accept",
            "value": "application/json",
            "type": "text"
          },
          {
            "key": "Content-Type",
            "value": "application/json",
            "type": "text"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\r\n  \"id\": {{petId}},\r\n  \"category\": {\r\n    \"id\": {{$randomInt}},\r\n    \"name\": \"{{$randomCatchPhraseDescriptor}}\"\r\n  },\r\n  \"name\": \"{{petName}}\",\r\n  \"photoUrls\": [\r\n    \"{{$randomImageUrl}}\"\r\n  ],\r\n  \"tags\": [\r\n    {\r\n      \"id\": {{$randomInt}},\r\n      \"name\": \"{{$randomWord}}\"\r\n    }\r\n  ],\r\n  \"status\": \"{{petStatus}}\"\r\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{baseUrl}}/pet",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "pet"
          ]
        }
      },
      "response": []
